SELECT * FROM EMP3;
SELECT * FROM DEPT2;

CROSS JOIN/CARTESIAN JOIN

SELECT * FROM EMP3, DEPT2;
      OR
SELECT * FROM EMP3 CROSS JOIN DEPT2;

INNER JOIN
SELECT ENAME,DNAME FROM EMP3 E, DEPT2 D WHERE E.DEPTNO = D.DEPTNO;
     OR
SELECT E.ENAME, D.DNAME FROM EMP3 E JOIN DEPT2 D ON E.DEPTNO = D.DEPTNO;

LEFT OUTER JOIN
SELECT ENAME,DNAME FROM EMP3,DEPT2 WHERE EMP3.DEPTNO = DEPT2.DEPTNO(+);

RIGHT OUTER JOIN
SELECT ENAME,DNAME FROM EMP3 E,DEPT2 D WHERE E.DEPTNO(+) = D.DEPTNO;

FULL JOIN
SELECT ENAME,DNAME FROM EMP3 E FULL JOIN DEPT2 D ON E.DEPTNO=D.DEPTNO;

NATURAL JOIN
SELECT ENAME,DNAME FROM EMP3 NATURAL JOIN DEPT2;




