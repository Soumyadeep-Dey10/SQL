SELECT * FROM EMP3;
SELECT * FROM DEPT2;

ASSIGNMENT ON HAVING CLAUSE
-------------------------------------------------------

SELECT DEPTNO,COUNT(*) FROM EMP3 GROUP BY DEPTNO HAVING COUNT(C)

SELECT DEPTNO, SUM(SAL)FROM EMP3 GROUP BY DEPTNO HAVING COUNT(*) >= 4;
 
SELECT JOB, COUNT(*) AS EMP_COUNT, SUM(SAL) AS TOTAL_SALARY FROM EMP3 WHERE SAL > 1200 GROUP BY JOB HAVING SUM(SAL) > 3800;

SELECT DEPTNO,COUNT(*) FROM EMP3 WHERE JOB='MANAGER'GROUP BY DEPTNO HAVING COUNT(*) = 2; 

SELECT JOB,MAX(SAL) FROM EMP3 GROUP BY JOB HAVING MAX(SAL)>2600;

SELECT SAL FROM EMP3 GROUP BY SAL HAVING COUNT(*) > 1;

SELECT HIREDATE FROM EMP3 GROUP BY HIREDATE HAVING COUNT(*) > 1;

SELECT AVG(SAL) FROM EMP3 GROUP BY DEPTNO HAVING AVG(SAL) < 3000;

SELECT DEPTNO FROM EMP3 WHERE ENAME LIKE '%A%' OR ENAME LIKE '%S%' GROUP BY DEPTNO HAVING COUNT(*) >= 3;

SELECT MIN(SAL),MAX(SAL) FROM EMP3 GROUP BY JOB HAVING MIN(SAL) > 1000 AND  MAX(SAL) < 5000;


ASSIGNMENT CASE - 1 
------------------------------------
SELECT ENAME FROM EMP3 WHERE SAL > (SELECT SAL FROM  EMP3 WHERE ENAME = 'ADAMS');

SELECT ENAME, SAL FROM EMP3 WHERE SAL < (SELECT SAL FROM  EMP3 WHERE ENAME = 'KING');

SELECT ENAME,DEPTNO FROM EMP3 WHERE DEPTNO = (SELECT DEPTNO FROM EMP3 WHERE ENAME = 'JONES');

SELECT ENAME,JOB FROM EMP3 WHERE JOB = (SELECT JOB FROM EMP3 WHERE ENAME = 'JAMES');

SELECT EMPNO,ENAME, (SAL * 12) FROM EMP3 WHERE (SAL *12) > (SELECT (SAL*12) FROM EMP3 WHERE ENAME = 'WARD');

SELECT ENAME, HIREDATE FROM EMP3 WHERE HIREDATE < (SELECT HIREDATE FROM EMP3 WHERE ENAME = 'SCOTT');

SELECT ENAME, HIREDATE FROM EMP3 WHERE HIREDATE > (SELECT HIREDATE FROM EMP3 WHERE JOB = 'PRESIDENT');

SELECT ENAME, SAL FROM EMP3 WHERE SAL < (SELECT SAL FROM EMP3 WHERE EMPNO = 7839);

SELECT * FROM EMP3 WHERE HIREDATE < (SELECT HIREDATE FROM EMP3 WHERE ENAME = 'MILLER');

SELECT ENAME, EMPNO FROM EMP3 WHERE SAL > (SELECT SAL FROM EMP3 WHERE ENAME = 'ALLEN');

SELECT ENAME, EMPNO FROM EMP3 WHERE SAL > (SELECT SAL FROM EMP3 WHERE ENAME = 'MILLER') AND SAL < (SELECT SAL FROM EMP3 WHERE ENAME = 'ALLEN');

SELECT * FROM EMP3 WHERE DEPTNO = 20 AND JOB = (SELECT JOB FROM EMP3 WHERE ENAME = 'SMITH');

SELECT * FROM Emp3 WHERE JOB = 'MANAGER' AND DEPTNO = (SELECT DEPTNO FROM Emp3 WHERE ENAME = 'TURNER');

SELECT ENAME,HIREDATE FROM EMP3 WHERE HIREDATE > '01-JAN-81' AND HIREDATE < (SELECT HIREDATE FROM EMP3 WHERE ENAME = 'KING'); 

SELECT ENAME, SAL, (SAL * 12) AS ANNUAL_SAL FROM Emp3 WHERE SAL < (SELECT SAL FROM Emp3 WHERE ENAME = 'BLAKE') AND SAL > 3500;

SELECT * FROM EMP3 WHERE SAL > (SELECT SAL FROM EMP3 WHERE ENAME = 'SCOTT') AND SAL < (SELECT SAL FROM EMP3 WHERE ENAME = 'KING');

SELECT ENAME FROM EMP3 WHERE ENAME LIKE 'A%' AND DEPTNO = (SELECT DEPTNO FROM EMP3   WHERE ENAME = 'BLAKE');

SELECT ENAME,COMM FROM EMP3 WHERE COMM IS NOT NULL AND JOB = (SELECT JOB FROM EMP3 WHERE ENAME = 'SMITH');

SELECT * FROM EMP3 WHERE JOB = 'CLERK' AND DEPTNO = (SELECT DEPTNO FROM Emp3 WHERE ENAME = 'TURNER');

SELECT ENAME, SAL, JOB FROM EMP3 WHERE (SAL * 12) > (SELECT SAL * 12 FROM EMP3 WHERE ENAME = 'SMITH') AND (SAL * 12) < (SELECT SAL * 12 FROM EMP3 WHERE ENAME = 'KING');



