JAMES      01-MAY-81
FORD       02-APR-81

5. WAQTD EMP NAME AND MANAGER'S COMM IF EMPLOYEE WORKS AS SALESMAN AND MANAGER WORKS IN DEPT 30

SELECT E.ENAME, M.COMM FROM EMP3 E,EMP3 M WHERE E.MGR=M.EMPNO AND E.JOB='SALESMAN' AND M.DEPTNO=30; 

